{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry"
      }
    ],
    "react-app"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    ["@babel/plugin-proposal-private-methods", { "loose": true }],
    [
      "babel-plugin-import",
      {
        "libraryName": "@alifd/next",
        "style": true
      }
    ]
  ]
}
